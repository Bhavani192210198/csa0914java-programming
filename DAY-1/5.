public class MatrixExample {
  public static void main(String[] args) {
    int[][] matrixA = new int[9][1];
    int[][] matrixB = new int[9][1];

    // First loop from 1 to 9
    for (int i = 0; i < 9; i++) {
      matrixA[i][0] = i + 1;
      System.out.print(matrixA[i][0] + " ");
    }
    System.out.println();

    // Second loop from 9 to 1
    for (int i = 8; i >= 0; i--) {
      matrixB[i][0] = i + 1;
      System.out.print(matrixB[i][0] + " ");
    }
    System.out.println();
  }
}
