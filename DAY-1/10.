public class GreatestOfThree {
  public static void main(String[] args) {
    int a = 10;
    int b = 20;
    int c = 30;

    int max = a;
    if (b > max) {
      max = b;
      if (c > max) {
        max = c;
      }
    } else if (c > max) {
      max = c;
    }

    System.out.println("The greatest of the three numbers is: " + max);
  }
}
